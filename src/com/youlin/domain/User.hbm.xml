<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="com.youlin.domain.User" table="user">
    <id name="userid" column="userid">
    <generator class="native"></generator>
    </id>
    <property name="username" column="username"/>
    <property name="password" column="password"></property>
    <property name="icopath" column="icopath"></property>
    <property name="sex" column="sex"></property>
    <property name="nickname" column="nickname"></property>
    <property name="birthday" column="birthday"></property>
    <property name="registerday" column="registerday"></property>
    <property name="phonenubmer" column="phonenubmer"></property>
    <property name="qqnumber" column="qqnumber"></property>
    <property name="weichat" column="weichat"></property>
    <property name="permanentaddress" column="permanentaddress"></property>
    
    </class>
    </hibernate-mapping>